# Months - only three-letter code is used
MONTH \b(?:Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec?)\b

# Days - two digit number is used
DAY \d{1,2}

# Time - two digit hour, minute, and second
TIME \d{2}:\d{2}:\d{2}

# Timestamp - month, day, and time
TIMESTAMP %{MONTH:UNWANTED}\s{1,2}%{DAY:UNWANTED} %{TIME:UNWANTED}

# Source
SOURCE .*

# Log start - the first part of the log line
LOGSTART %{TIMESTAMP:timestamp_string} %{SOURCE:hostname} %{SOURCE:program}

# Log Message
MESSAGE .*

# Log end - the level of the log and the message
LOGEND %{MESSAGE:log_message}

# SecureLinux - a basic SecureLinux log entry
SECURELINUX %{LOGSTART:UNWANTED}: %{LOGEND:UNWANTED}


# Log Message Patterns
SECURETIMESTAMP %{MONTH:UNWANTED}%{SPACE:UNWANTED}%{MONTHDAY:UNWANTED} %{TIME:UNWANTED}
SECUREPROG %{PROG:program}(?:\[%{POSINT:pid}\])?
SECUREHOST %{IPORHOST:host}
SECUREBASE %{SECURETIMESTAMP:secure_timestamp}%{SPACE:UNWANTED}%{SECUREHOST:UNWANTED}%{SPACE:UNWANTED}%{SECUREPROG:UNWANTED}:%{SPACE:UNWANTED}%{GREEDYDATA:log_message}


MESSAGE1 \[origin software=\"%{DATA:software_name}\" swVersion=\"%{DATA:software_version}\" x\-pid=\"%{POSINT:pid}\" x\-info=\"%{DATA:software_info}\"\] %{WORD:UNWANTED} was %{WORD:action}
MESSAGE2 %{WORD:action} on %{DATA:adapter} to %{IPORHOST:src_ip} port %{POSINT:port} interval %{POSINT:interval}
TRANSACTION <%{WORD:log_level}> \(%{WORD:adapter}\): %{WORD:action} %{WORD:application} transaction, %{WORD:UNWANTED} client pid %{POSINT:pid}
MESSAGE3 <%{WORD:log_level}> \(%{WORD:adapter}\): %{WORD:action} %{GREEDYDATA:warning}.
ACTIVATION <%{WORD:log_level}> Activation \(%{WORD:adapter}\) Stage %{POSINT:stage_current} of %{POSINT:stage_total} \(%{DATA:protocol} %{GREEDYDATA:description}\) %{WORD:action}...
STATECHANGE <%{WORD:log_level}> \(%{WORD:adapter}\): %{WORD:changed} state change: %{DATA:change} -> %{WORD:result} \(reason '%{DATA:reason}'\) [%{GREEDYDATA:UNWANTED]
CONNECTION <%{WORD:log_level}> Marking connection '%{DATA:connection}' %{WORD:status}.
ACTIVATIONFAILED <%{WORD:log_level}> Activation \(%{WORD:adapter}\) failed.
POLICYSET <%{WORD:log_level}> Policy set \'%{GREEDYDATA:description}\' \(%{WORD:adapter}\) as %{WORD:policy} for %{GREEDYDATA:policy_for}.
LISTENING Listening on %{GREEDYDATA:adapter}